{







  "compilerOptions": {







    "target": "ES2020",







    "module": "NodeNext",







    "moduleResolution": "NodeNext",







    "esModuleInterop": true,







    "allowSyntheticDefaultImports": true,







    "strict": true,







    "skipLibCheck": true,







    "forceConsistentCasingInFileNames": true,







    "outDir": "dist",







    "rootDir": ".",







    "baseUrl": ".",







    "paths": {







      "*": ["node_modules/*"]







    },







    "lib": ["ES2020", "DOM"],







    "downlevelIteration": true,







    "resolveJsonModule": true







  },







  "include": ["./**/*.ts"],







  "exclude": ["node_modules", "dist"]







} 






